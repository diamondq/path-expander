<project xmlns="http://maven.apache.org/POM/4.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>com.diamondq</groupId>
    <artifactId>diamondq-parent</artifactId>
    <version>[6.1-SNAPSHOT,7)</version>
    <relativePath>../../common-maven/diamondq-parent</relativePath>
  </parent>
  <groupId>com.diamondq.path-expander</groupId>
  <artifactId>path-expander-masterparent</artifactId>
  <version>0.1.0-SNAPSHOT</version>
  <packaging>pom</packaging>
  <name>DiamondQ Path Expander: Master Parent</name>
  <description>DiamondQ Path Expander</description>
  <properties>
    <scm.project>path-expander</scm.project>
    <nexus.url>https://nexus.diamondq.com</nexus.url>
    <nexus.serverId>1-dq-snapshot</nexus.serverId>
    <nexus.auto.release>false</nexus.auto.release>
    <nexus.skip.staging>true</nexus.skip.staging>
  </properties>
  <distributionManagement>
    <snapshotRepository>
      <id>1-dq-snapshot</id>
      <url>https://nexus.diamondq.com/repository/maven-snapshots/</url>
    </snapshotRepository>
    <repository>
      <id>2-dq-release</id>
      <url>https://nexus.diamondq.com/repository/maven-releases/</url>
    </repository>
  </distributionManagement>
  <profiles>
    <profile>
      <id>diamondq-jenkins</id>
      <activation>
        <property>
          <name>jenkins</name>
          <value>true</value>
        </property>
      </activation>
      <repositories>
        <repository>
          <id>1-dq-snapshot</id>
          <url>https://nexus.diamondq.com/repository/maven-snapshots/</url>
          <snapshots>
            <enabled>true</enabled>
            <updatePolicy>always</updatePolicy>
          </snapshots>
          <releases>
            <enabled>false</enabled>
          </releases>
        </repository>
        <repository>
          <id>2-dq-release</id>
          <url>https://nexus.diamondq.com/repository/maven-public/</url>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
        </repository>
        <repository>
          <id>ossrh-snap</id>
          <url>https://oss.sonatype.org/content/repositories/snapshots</url>
          <snapshots>
            <enabled>true</enabled>
            <updatePolicy>always</updatePolicy>
          </snapshots>
          <releases>
            <enabled>false</enabled>
          </releases>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <id>ossrh-snap</id>
          <url>https://oss.sonatype.org/content/repositories/snapshots</url>
          <snapshots>
            <enabled>true</enabled>
            <updatePolicy>always</updatePolicy>
          </snapshots>
          <releases>
            <enabled>false</enabled>
          </releases>
        </pluginRepository>
      </pluginRepositories>
    </profile>
    <profile>
      <id>diamondq-default</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <repositories>
        <repository>
          <id>1-dq-snapshot</id>
          <url>https://nexus.diamondq.com/repository/maven-snapshots/</url>
          <snapshots>
            <enabled>true</enabled>
            <updatePolicy>daily</updatePolicy>
          </snapshots>
          <releases>
            <enabled>false</enabled>
          </releases>
        </repository>
        <repository>
          <id>2-dq-release</id>
          <url>https://nexus.diamondq.com/repository/maven-public/</url>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
        </repository>
        <repository>
          <id>ossrh-snap</id>
          <url>https://oss.sonatype.org/content/repositories/snapshots</url>
          <snapshots>
            <enabled>true</enabled>
            <updatePolicy>daily</updatePolicy>
          </snapshots>
          <releases>
            <enabled>false</enabled>
          </releases>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <id>ossrh-snap</id>
          <url>https://oss.sonatype.org/content/repositories/snapshots</url>
          <snapshots>
            <enabled>true</enabled>
            <updatePolicy>daily</updatePolicy>
          </snapshots>
          <releases>
            <enabled>false</enabled>
          </releases>
        </pluginRepository>
      </pluginRepositories>
    </profile>
  </profiles>
</project>